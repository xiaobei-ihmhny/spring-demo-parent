<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="chinese" class="com.xiaobei.spring.demo.dependency.domain.Lesson">
        <property name="id" value="45"/>
        <property name="name" value="语文"/>
    </bean>

    <bean id="maths" class="com.xiaobei.spring.demo.dependency.domain.Lesson">
        <property name="id" value="45"/>
        <property name="name" value="数学"/>
    </bean>

    <bean id="history" class="com.xiaobei.spring.demo.dependency.domain.Lesson">
        <property name="id" value="45"/>
        <property name="name" value="历史"/>
    </bean>

    <bean class="com.xiaobei.spring.demo.dependency.domain.LessonPrototype">
        <property name="id" value="45"/>
        <property name="name" value="computer"/>
    </bean>

</beans>